/*公有样式*/
.flex_vert{
  display: flex;
  flex-direction: column;

}
.container{
  height: 100vh;
  overflow: hidden;
  background:white;
}
.other{
  height: 300rpx;
  margin-bottom: 20rpx;
  background: lightgray;
}
.notes{
  flex: 1;
  background:black;
  overflow: hidden;
}
.notes_inner{
  text-align: center;
}
.notes_col{
  width: 46.5%;
  height: 300rpx;
  background:red;
  display: inline-block;
}
.notes_col-left{
  background: yellow;
  margin-right: 2.6%;
}